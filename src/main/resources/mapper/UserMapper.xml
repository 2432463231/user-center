<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.user.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.user.entity.User">
        <id column="id" property="id" />
        <result column="username" property="username" />
        <result column="userAccount" property="useraccount" />
        <result column="avatarUrl" property="avatarurl" />
        <result column="gender" property="gender" />
        <result column="userPassword" property="userpassword" />
        <result column="phone" property="phone" />
        <result column="email" property="email" />
        <result column="userStatus" property="userstatus" />
        <result column="createTime" property="createtime" />
        <result column="updateTime" property="updatetime" />
        <result column="isDelete" property="isdelete" />
        <result column="userRole" property="userrole" />
        <result column="planetCode" property="planetcode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        username, id, userAccount, avatarUrl, gender, userPassword, phone, email, userStatus, createTime, updateTime, isDelete, userRole, planetCode
    </sql>

</mapper>
